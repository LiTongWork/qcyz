/* pages/store/tenants/tenants.wxss */
.list {
  margin-top: 25rpx;
  width: 100%;
  background-color: #fff;
}
.list-item {
  width: 660rpx;
  min-height: 100rpx;
  padding: 0 45rpx;
  border-bottom: 1px solid #e9e9e9;
  display: flex;
  justify-content: space-between;
}
.list-item .title, .list-item .input  {
  height: 100rpx;
  line-height: 100rpx;
}
.list-item .input {
  flex: 1;
  padding: 0 20rpx;
}
.list-item .input input {
  display: block;
  height: 100%;
}
/* 商品类别选择 */
.category .input {
  height: auto;
  display: flex;
  flex-wrap: wrap;
}
.select {
  width: auto;
  padding: 0 10rpx;
  display: flex;
  align-items: center;
}
.select image{
  width: 42rpx;
  margin-right: 10rpx;
}
/* 提交审核 */
.sub {
  margin-top: 180rpx;
  width: 100%;
}
.sub button {
  margin: 0 auto;
  width: 660rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #fff;
  background: linear-gradient(to right, #ffcf00, #ffb302);
}